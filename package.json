{
  "name": "Boilerplate",
  "description": "Boilerplate framework",
  "version": "0.0.2",
  "private": true,
  "author": "Tim Roussilhe",
  "browserslist": "last 3 versions, iOS 8",
  "contributors": [
    "Tim Roussilhe"
  ],
  "app": {},
  "engines": {
    "node": "latest",
    "npm": "latest"
  },
  "scripts": {
    "dist:sass": "node-sass --include-path node_modules ./src/css/app.scss ./build/assets/css/app.min.css",
    "dist:autoprefixer": "postcss -c postcss.config.js ./build/assets/css/app.min.css --replace --no-map",
    "dist:js": "webpack --config ./config/webpack.prod.config.js ",
    "lint:css": "stylelint ./build/assets/css/*.css",
    "lint:js": "eslint ./src/scripts/project",
    "watch:js": "webpack --config ./config/webpack.dev.config.js --progress --colors --cache",
    "svgs-to-jsons": "gulp svgs-to-jsons",
    "server": "php -S 127.0.0.1:8000 -t public/",
    "browsersync": "gulp browserSync",
    "build": "gulp build",
    "dev": "concurrently -p \"[{name}]\" -n \"WEBPACK\" -c \"bgBlue.bold,bgRed.bold\" \"npm run watch:js\"",
    "dev:sass": "gulp sass",
    "start": "npm run server & npm run dev:sass && npm run browsersync && npm run watch:js",
    "plato:develop": "node plato",
    "plato:watch": "node plato/watch.js",
    "plato:deploy": "node plato build",
    "develop-OLD": "npm run plato:develop && npm run dev:sass && (npm run browsersync & npm run plato:watch & npm run watch:js)",
    "develop": "npm run plato:develop && ( npm run plato:watch & npm run watch:js)",
    "deploy-OLD": "npm run dist:sass && npm run dist:autoprefixer && npm run dist:js && npm run lint:js && npm run build ",
    "serve": "webpack-dev-server --config ./config/webpack.dev.config.js",
    "deploy": "npm run plato:deploy"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.26.3",
    "chokidar": "^2.0.4",
    "concurrently": "^3.6.0",
    "css-loader": "^2.0.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^7.0.1",
    "gsap": "^2.0.2",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-connect-php": "^1.0.3",
    "gulp-files-to-json": "^0.2.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.2.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svgmin": "^1.2.4",
    "gulp-watch": "^5.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "imports-loader": "^0.8.0",
    "lodash": "^4.17.10",
    "lodash-es": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "node-notifier": "^5.2.1",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.0",
    "nunjucks": "^3.1.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "page": "^1.8.6",
    "postcss-loader": "^3.0.0",
    "redux": "^4.0.0",
    "redux-responsive": "^4.3.7",
    "redux-watch": "^1.1.1",
    "run-sequence": "^2.2.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "twig-loader": "^0.4.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.27.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.10",
    "webpack-manifest-plugin": "^2.0.3",
    "whatwg-fetch": "^3.0.0",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "bluebird": "^3.5.3",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "commander": "^2.19.0",
    "critical": "^1.3.4",
    "hoek": "^5.0.4",
    "html-minifier": "^3.5.21",
    "node-fetch": "^2.3.0",
    "opn": "^5.4.0",
    "redux-logger": "^3.0.6",
    "time-require": "^0.1.2",
    "twig": "^1.12.0",
    "valid-url": "^1.0.9"
  }
}
