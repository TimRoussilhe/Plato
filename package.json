{
  "name": "plato",
  "description": "Plato Boilerplate",
  "version": "0.0.2",
  "private": true,
  "author": "Tim Roussilhe",
  "browserslist": [
    "last 3 versions",
    "iOS 9",
    "Safari >= 10",
    "not ie <= 9"
  ],
  "contributors": [
    "Tim Roussilhe"
  ],
  "app": {},
  "engines": {
    "node": "17.4.0",
    "npm": "8.4.0"
  },
  "scripts": {
    "clean": "rm -rf node_modules/.cache/hard-source",
    "start": "npm run clean &&  node plato develop -v -o",
    "plato:develop": "node plato develop",
    "plato:deploy": "node plato build",
    "develop": "npm run clean && node plato develop -v",
    "serve": "webpack-dev-server --config ./config/webpack.dev.config.js",
    "build": "node plato build -v",
    "build-and-test": "node plato build -v -o",
    "deploy": "node plato build",
    "bundleSizeAnalyser": "webpack --config ./config/webpack.prod.config.js --env bundleSize",
    "prettier:check": "prettier --config ./.prettierrc --check src/**/**.{js,scss}",
    "prettier:write": "prettier --config ./.prettierrc --write src/**/**.{js,scss}"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "art-template-loader": "^1.4.3",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.0.4",
    "chokidar": "^3.5.3",
    "critical": "^4.0.1",
    "css-loader": "^6.7.1",
    "eslint": "^8.21.0",
    "eslint-config-google": "^0.14.0",
    "fs-extra": "^10.1.0",
    "gsap": "^3.10.4",
    "mini-css-extract-plugin": "^2.6.1",
    "page": "^1.11.6",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.7.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-responsive": "^4.3.8",
    "redux-watch": "^1.1.1",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "size-plugin": "^3.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-bundle-size-analyzer": "^3.1.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-manifest-plugin": "^5.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@babel/runtime": "^7.6.2",
    "art-template": "^4.13.2",
    "babel-eslint": "^10.0.1",
    "boxen": "^4.1.0",
    "commander": "^2.19.0",
    "connect": "^3.7.0",
    "convert-hrtime": "^2.0.0",
    "core-js": "^3.2.1",
    "eslint-config-prettier": "^6.3.0",
    "hoek": "^6.1.2",
    "html-minifier": "^3.5.21",
    "lodash": "^4.17.19",
    "mkdirp": "^1.0.4",
    "node-fetch": "^2.3.0",
    "opn": "^5.4.0",
    "serve-static": "^1.14.1",
    "terminal-link": "^2.0.0"
  }
}
