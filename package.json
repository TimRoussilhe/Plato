{
  "name": "plato",
  "description": "Plato Boilerplate",
  "version": "0.0.2",
  "private": true,
  "author": "Tim Roussilhe",
  "browserslist": [
    "last 3 versions",
    "iOS 9",
    "Safari >= 10",
    "not ie <= 9"
  ],
  "contributors": [
    "Tim Roussilhe"
  ],
  "app": {},
  "engines": {
    "node": "latest",
    "npm": "latest"
  },
  "scripts": {
    "clean": "rm -rf node_modules/.cache/hard-source",
    "start": "npm run clean &&  node plato develop -v -o",
    "plato:develop": "node plato develop",
    "plato:deploy": "node plato build",
    "develop": "npm run clean && node plato develop -v",
    "serve": "webpack-dev-server --config ./config/webpack.dev.config.js",
    "build": "node plato build -v",
    "build-and-test": "node plato build -v -o",
    "deploy": "node plato build",
    "bundleSizeAnalyser": "webpack --config ./config/webpack.prod.config.js --env bundleSize",
    "prettier:check": "prettier --config ./.prettierrc --check src/**/**.{js,scss}",
    "prettier:write": "prettier --config ./.prettierrc --write src/**/**.{js,scss}"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.6.2",
