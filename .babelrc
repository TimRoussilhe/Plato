{
	"presets": [
		[
			"@babel/env",
			{
				"targets": {
					"browsers": ["last 2 versions", "ie >= 10"]
				},
				"useBuiltIns": "usage",
				"corejs": { "version": 3, "proposals": true },
				"modules": false
			}
		]
	],
	"plugins": ["@babel/plugin-syntax-dynamic-import", "@babel/plugin-transform-runtime"]
}
